{
    "name": "Gmail Classic",
    "version": "1.0",
    "description": "Revert my gmail",
    "manifest_version": 2,
    "web_accessible_resources": [
      	"classic.css"
    ],
    "background":{
        "scripts":["jquery-3.3.1.min.js"]
    },
	"content_scripts": [
		{
		  "matches": ["https://mail.google.com/mail/*"],
		  "js": ["jquery-3.3.1.min.js","classic.js"],
		  "run_at":"document_start"
		}
	]
}